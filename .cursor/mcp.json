{
  "mcpServers": {
    "envFile": ".env.cursor",
    "context7": {
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "${env:CURSOR_CONTEXT7_API_KEY}"
      }
    },
    "Atlassian": {
      "command": "npx mcp-remote https://mcp.atlassian.com/v1/sse",
      "env": {},
      "args": []
    },
    "GitHub": {
      "command": "docker run -i --rm -e GITHUB_PERSONAL_ACCESS_TOKEN ghcr.io/github/github-mcp-server",
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${env:CURSOR_GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "args": []
    },
    "Vercel": {
      "url": "https://mcp.vercel.com",
      "headers": {}
    }
  }
}
